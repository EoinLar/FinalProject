<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.eoinl.fantasygaa.userTeamActivity">

    <Button
            android:text="LFF"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/btnLFF"
            android:layout_marginLeft="16dp" app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/btnFF" android:layout_marginRight="16dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/btnLHF"
            app:layout_constraintHorizontal_bias="0.505"/>
    <Button
            android:text="RHF"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/button33"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent" app:layout_constraintLeft_toRightOf="@+id/btnCF"
            android:layout_marginLeft="16dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btnRM"/>
    <Button
            android:text="LHB"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/button32"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent" app:layout_constraintRight_toLeftOf="@+id/btnCB"
            android:layout_marginRight="16dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button31" app:layout_constraintHorizontal_bias="0.505"/>
    <Button
            android:text="LFB"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/button31"
            app:layout_constraintRight_toLeftOf="@+id/button30" android:layout_marginRight="16dp"
            android:layout_marginLeft="16dp" app:layout_constraintLeft_toLeftOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btnGK" app:layout_constraintHorizontal_bias="0.505"/>
    <Button
            android:text="FB"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/button30"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/btnGK"
            android:layout_marginRight="16dp" app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="16dp" app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.501"/>
    <Button
            android:text="CF"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/btnCF"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btnRM" android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent" android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent" app:layout_constraintHorizontal_bias="0.501"/>
    <Button
            android:text="RFF"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/btnRFF"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button33" android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent" app:layout_constraintLeft_toRightOf="@+id/btnFF"
            android:layout_marginLeft="16dp"/>
    <Button
            android:text="FF"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/btnFF"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent" android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btnCF" app:layout_constraintHorizontal_bias="0.501"/>
    <Button
            android:text="LHF"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/btnLHF"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btnLM" android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent" app:layout_constraintRight_toLeftOf="@+id/btnCF"
            android:layout_marginRight="16dp"/>
    <Button
            android:text="LM"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/btnLM"
            app:layout_constraintLeft_toRightOf="@+id/button32" android:layout_marginLeft="16dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/btnRM"/>
    <Button
            android:text="RM"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/btnRM"
            app:layout_constraintRight_toLeftOf="@+id/btnRHB" android:layout_marginRight="16dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/btnCB"/>
    <TextView
            android:text="User Name"
            android:layout_width="365dp"
            android:layout_height="41dp"
            android:id="@+id/userTeamName"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent" android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"/>
    <Button
            android:text="CB"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/btnCB"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent" android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button30" app:layout_constraintHorizontal_bias="0.501"/>
    <Button
            android:text="RFB"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/btnRFB"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent" app:layout_constraintLeft_toRightOf="@+id/button30"
            android:layout_marginLeft="16dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btnGK"/>
    <Button
            android:text="RHB"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:id="@+id/btnRHB"
            app:layout_constraintLeft_toRightOf="@+id/btnCB"
            android:layout_marginLeft="16dp" android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btnRFB"/>
    <Button
            android:text="GK"
            android:layout_width="49dp"
            android:layout_height="wrap_content"
            android:id="@+id/btnGK"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/userTeamName" android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent" android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.501"/>

</android.support.constraint.ConstraintLayout>
